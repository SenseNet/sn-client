{
  "name": "@sensenet/controls-react",
  "version": "2.3.7",
  "description": "React controls for sensenet",
  "main": "dist/index.js",
  "scripts": {
    "fix": "npm run fix:prettier && npm run fix:tslint",
    "fix:prettier": "prettier \"{,!(dist|temp)/**/}*.{ts,tsx}\" --write",
    "fix:tslint": "tslint --fix --project .",
    "lint": "tslint --project .",
    "build": "tsc -b && cpx \"src/**/*.css\" dist",
    "typedoc": "typedoc --mode modules --out documentation src --exclude **/index.ts --theme c:/Users/%USERNAME%/AppData/Roaming/npm/node_modules/@sensenet/typedoc-theme/sn-theme",
    "precommit": "npm run build",
    "commit": "git-cz"
  },
  "files": [
    "dist",
    "src",
    "test",
    "documentation",
    "coverage"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SenseNet/sn-client.git"
  },
  "keywords": [
    "sensenet",
    "ecm",
    "react",
    "reactjs",
    "controls",
    "components"
  ],
  "author": "SenseNet",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/SenseNet/sn-client/issues"
  },
  "homepage": "https://sensenet.com",
  "dependencies": {
    "@date-io/moment": "1.1.0",
    "@material-ui/core": "^3.6.1",
    "@sensenet/client-core": "^1.4.0",
    "@sensenet/control-mapper": "^1.0.4",
    "@sensenet/icons-react": "^1.2.5",
    "@sensenet/redux": "^5.1.4",
    "lodash.debounce": "^4.0.8",
    "material-ui-pickers": "^2.2.1",
    "moment": "^2.24.0",
    "radium": "^0.25.0",
    "react-quill": "^1.3.1",
    "react-responsive": "^6.0.1",
    "react-select": "^2.0.0"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.4",
    "@types/node": "^10.10.3",
    "@types/radium": "^0.24.2",
    "@types/react": "^16.7.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.11",
    "@types/react-responsive": "^3.0.2",
    "autoprefixer": "^9.1.5",
    "chalk": "2.4.2",
    "cpx": "^1.5.0",
    "date-fns": "^2.0.0-alpha.21",
    "dotenv": "6.2.0",
    "fs-extra": "^7.0.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "promise": "8.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.7.0",
    "react-dev-utils": "^7.0.1",
    "react-dom": "^16.7.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "typings": "./dist/index.d.ts"
}
