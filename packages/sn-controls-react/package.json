    "ts-loader": "^2.0.3",
    "tslint": "^5.2.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.0.0",
    "typedoc": "^0.8.0",
    "typedoc-default-themes": "^0.5.0",
    "typedoc-plugin-external-module-name": "^1.0.9",
    "typescript": "^2.4.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-manifest-plugin": "1.3.1",
    "whatwg-fetch": "2.0.3"
  },
  "dependencies": {
    "@reactivex/rxjs": "^5.4.2",
    "material-ui": "^0.19.0",
    "material-ui-chip-input": "^0.17.0",
    "nyc": "^11.0.3",
    "radium": "^0.19.1",
