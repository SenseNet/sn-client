{
  "name": "@sensenet/controls-react",
  "version": "2.6.0",
  "description": "React controls for sensenet",
  "main": "dist/index.js",
  "scripts": {
    "fix": "npm run fix:prettier && npm run fix:tslint",
    "fix:prettier": "prettier \"{,!(dist|temp)/**/}*.{ts,tsx}\" --write",
    "fix:tslint": "tslint --fix --project .",
    "lint": "tslint --project .",
    "build": "tsc -b",
    "typedoc": "typedoc --mode modules --out documentation src --exclude **/index.ts --theme c:/Users/%USERNAME%/AppData/Roaming/npm/node_modules/@sensenet/typedoc-theme/sn-theme",
    "precommit": "npm run build",
    "commit": "git-cz"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SenseNet/sn-client.git"
  },
  "keywords": [
    "sensenet",
    "ecm",
    "react",
    "reactjs",
    "controls",
    "components"
  ],
  "author": "SenseNet",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/SenseNet/sn-client/issues"
  },
  "homepage": "https://sensenet.com",
  "dependencies": {
    "@date-io/moment": "1.1.0",
    "@material-ui/core": "^3.6.1",
    "@sensenet/client-core": "^2.0.0",
    "@sensenet/control-mapper": "^1.1.2",
    "@sensenet/icons-react": "^1.2.9",
    "@sensenet/pickers-react": "^1.1.0",
    "lodash.debounce": "^4.0.8",
    "material-ui-pickers": "^2.2.1",
    "moment": "^2.24.0",
    "radium": "^0.25.0",
    "react-quill": "^1.3.1",
    "react-responsive": "^6.0.1",
    "react-select": "^2.0.0"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^11.10.4",
    "@types/radium": "^0.24.2",
    "@types/react": "^16.7.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-responsive": "^3.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "typings": "./dist/index.d.ts"
}
