{
  "name": "@sensenet/icons-react",
  "version": "1.2.3",
  "description": "A React component rendering icons for sensenet.",
  "main": "dist/index.js",
  "scripts": {
    "precommit": "npm run test",
    "commit": "git-cz",
    "pretest": "rimraf temp && tsc -p tsconfig.test.json  && cpx \"src/assets/**/*.*\" temp/src/assets",
    "test": "rimraf coverage && nyc mocha --require ignore-styles -p tsconfig.test.json ./temp/test/index.js",
    "check-coverage": "istanbul check-coverage ",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "fix": "npm run fix:prettier && npm run fix:tslint",
    "fix:prettier": "prettier \"{,!(dist|temp|bundle)/**/}*.{ts,tsx}\" --write",
    "fix:tslint": "tslint --fix --project .",
    "lint": "tslint --project .",
    "prebuild": "npm run lint",
    "build": "rimraf dist && tsc -p tsconfig.json && cpx \"src/assets/**/*.*\" dist/assets",
    "prepublishOnly": "npm run test && npm run build",
    "publish:development": "npm run build && npm t && npm run typedoc && npm publish --tag development",
    "typedoc": "typedoc --mode modules --out documentation src --exclude **/index.ts --theme c:/Users/%USERNAME%/AppData/Roaming/npm/node_modules/@sensenet/typedoc-theme/sn-theme"
  },
  "files": [
    "dist",
    "src",
    "test",
    "documentation",
    "coverage"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SenseNet/sn-client.git"
  },
  "keywords": [
    "sensenet",
    "ecm",
    "react",
    "reactjs",
    "icons",
    "components"
  ],
  "author": "SenseNet",
  "license": "GPL-2.0",
  "nyc": {
    "cache": true,
    "extension": [
      ".tsx",
      ".ts",
      ".d.ts"
    ],
    "include": "temp/src/components/**/*.*",
    "check-coverage": true,
    "exclude": [
      "**/*.css",
      "temp/test/**/*.*",
      "**/index.js"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "json"
    ]
  },
  "bugs": {
    "url": "https://github.com/SenseNet/sn-icons-react/issues"
  },
  "homepage": "https://sensenet.com",
  "dependencies": {
    "@material-ui/core": "^1.1.0",
    "@material-ui/icons": "^1.0.0-beta.42",
    "react-test-renderer": "^16.6.3"
  },
  "peerDependencies": {
    "react-dom":"^16.6.3",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.11",
    "@types/react": "^16.7.11",
    "@types/react-test-renderer": "^16.0.3",
    "codecov.io": "^0.1.6",
    "cpx": "^1.5.0",
    "ignore-styles": "^5.0.1",
    "react": "^16.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "sensenet-kfi-cz-conventional-changelog"
    }
  },
  "typings": "./dist/index.d.ts"
}
