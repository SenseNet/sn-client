{
  "name": "@sensenet/authentication-google",
  "version": "2.0.1",
  "description": "Client-side Google authentication provider for sensenet",
  "main": "dist/index.js",
  "scripts": {
    "precommit": "npm run test",
    "commit": "git-cz",
    "pretest": "tslint --project tsconfig.test.json && rimraf temp && tsc -p tsconfig.test.json",
    "test": "rimraf coverage && nyc mocha -p tsconfig.test.json ./temp/test/index.js",
    "prebuild": "npm run lint",
    "fix": "npm run fix:prettier && npm run fix:tslint",
    "fix:prettier": "prettier \"{,!(dist|temp)/**/}*.{ts,tsx}\" --write",
    "fix:tslint": "tslint --fix --project .",
    "lint": "tslint --project .",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "prepublishOnly": "npm run test && npm run build",
    "publish:development": "npm run build && npm t && npm run typedoc && npm publish --tag development",
    "typedoc": "typedoc --mode file --out documentation src --tsconfig tsconfig.json --theme c:/Users/%USERNAME%/AppData/Roaming/npm/node_modules/@sensenet/typedoc-theme/sn-theme"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SenseNet/sn-client-auth-google.git"
  },
  "keywords": [
    "sensenet",
    "ECM",
    "document",
    "management",
    "Google",
    "OAuth",
    "JWT",
    "Login"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Sense/Net <snteam@sensenet.com> (http://sensenet.com/)",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/SenseNet/sn-client-auth-google/issues"
  },
  "nyc": {
    "exclude": [
      "temp/test/**/*.*",
      "temp/src/**/I*.js"
    ],
    "include": "temp/src/**/*.*",
    "check-coverage": true,
    "cache": true,
    "all": true,
    "reporter": [
      "lcov",
      "text-summary",
      "json"
    ]
  },
  "homepage": "https://github.com/SenseNet/sn-client-auth-google#readme",
  "dependencies": {
    "@sensenet/authentication-jwt": "1.0.3",
    "@sensenet/client-utils":"^1.4.0"
  },
  "config": {
    "commitizen": {
      "path": "sensenet-kfi-cz-conventional-changelog"
    }
  },
  "typings": "./dist/index.d.ts",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/jsdom": "^12.2.0",
    "@types/mocha": "^5.2.0",
    "chai": "^4.1.2",
    "jsdom": "^13.0.0",
    "mocha": "^5.1.1",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.10.0",
    "typescript": "^3.1.6"
  }
}
