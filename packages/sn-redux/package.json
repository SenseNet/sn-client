{
  "name": "@sensenet/redux",
  "version": "5.1.3",
  "description": "A set of redux actions, reducers and redux-ovbservable epics for sensenet",
  "main": "dist/index.js",
  "scripts": {
    "fix": "npm run fix:prettier && npm run fix:tslint",
    "fix:prettier": "prettier \"{,!(dist|temp|bundle)/**/}*.{ts,tsx}\" --write",
    "fix:tslint": "tslint --fix --project .",
    "lint": "tslint --project .",
    "build": "tsc -b",
    "typedoc": "typedoc --mode modules --out documentation src --exclude **/index.ts --theme c:/Users/%USERNAME%/AppData/Roaming/npm/node_modules/@sensenet/typedoc-theme/sn-theme"
  },
  "files": [
    "dist",
    "src",
    "test",
    "documentation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SenseNet/sn-client.git"
  },
  "keywords": [
    "sensenet",
    "ecm",
    "redux",
    "crud"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Sense/Net <snteam@sensenet.com> (http://sensenet.com/)",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/SenseNet/sn-client/issues"
  },
  "homepage": "https://sensenet.com",
  "dependencies": {
    "@sensenet/authentication-google": "^2.0.2",
    "@sensenet/authentication-jwt": "^1.0.6",
    "@sensenet/client-core": "^1.3.2",
    "@sensenet/default-content-types": "^1.1.1",
    "@sensenet/redux-promise-middleware": "^2.1.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.14",
    "@types/redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3"
  },
  "config": {
    "commitizen": {
      "path": "sensenet-kfi-cz-conventional-changelog"
    }
  },
  "typings": "./dist/index.d.ts"
}
