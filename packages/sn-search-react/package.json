{
  "name": "@sensenet/search-react",
  "version": "1.1.2-alpha.0",
  "description": "",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "sensenet",
    "ecm",
    "search",
    "query"
  ],
  "scripts": {
    "precommit": "npm run test",
    "commit": "git-cz",
    "pretest": "rimraf temp && tsc -p tsconfig.test.json",
    "test": "rimraf coverage && nyc mocha -p tsconfig.test.json ./temp/test/index.js",
    "fix": "npm run fix:prettier && npm run fix:tslint",
    "fix:prettier": "prettier \"{,!(dist|temp|bundle)/**/}*.{ts,tsx}\" --write",
    "fix:tslint": "tslint --fix --project .",
    "lint": "tslint --project .",
    "build": "npm run build:tsc",
    "build:tsc": "tsc -b",
    "build:webpack": "rimraf bundle && webpack",
    "publish:development": "npm run build && npm t && npm run typedoc && npm publish --tag development",
    "prepublishOnly": "npm run test && npm run build",
    "typedoc": "typedoc --mode file --out documentation src --tsconfig tsconfig.json --theme c:/Users/%USERNAME%/AppData/Roaming/npm/node_modules/@sensenet/typedoc-theme/sn-theme",
    "start": "webpack-dev-server"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Sense/Net <snteam@sensenet.com> (http://sensenet.com/)",
  "license": "GPL-2.0",
  "nyc": {
    "lines": 80,
    "exclude": [
      "temp/test/**/*.*",
      "temp/**/index.js",
      "temp/src/style.js",
      "temp/src/**/I*.js",
      "temp/src/models/*.js",
      "temp/src/services/TypeHelpers.js",
      "temp/src/ExampleAppLayout.js",
      "**/index.js"
    ],
    "include": "temp/src/**/*.*",
    "check-coverage": false,
    "cache": true,
    "all": true,
    "reporter": [
      "lcov",
      "text-summary",
      "json"
    ]
  },
  "homepage": "https://sensenet.com",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/react": "^16.7.11",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.0.3",
    "@types/uuid": "^3.4.4",
    "autoprefixer": "^9.4.0",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.6.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "@sensenet/client-core": "^1.3.2-alpha.0",
    "@sensenet/control-mapper": "^1.0.3-alpha.0",
    "@sensenet/default-content-types": "^1.1.1-alpha.0",
    "@sensenet/icons-react": "^1.2.4-alpha.0",
    "@sensenet/query": "^1.1.1-alpha.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "config": {
    "commitizen": {
      "path": "sensenet-kfi-cz-conventional-changelog"
    }
  },
  "typings": "./dist/index.d.ts"
}
